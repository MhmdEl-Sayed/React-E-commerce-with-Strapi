{
    "compilerOptions": {
      "checkJs": true,
      "jsx": "react-jsx",
      "baseUrl": "src",
      "resolveJsonModule": true
    },
    "include": [
      "src"
    ],
    "exclude": [
      "node_modules",
      "build"
    ], 
    "plugins": ["react-hooks"],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
    
  }


 
   


  